    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "unimport": "^5.0.1"
  },
  "dependencies": {
    "ahooks": "^3.8.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "tailwindcss": "^4.1.10",
    "tdesign-react": "1.12.0"
  }
