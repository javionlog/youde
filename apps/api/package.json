{
  "name": "@youde/api",
  "version": "0.0.1",
  "author": "Javionlog",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.17.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start:dev": "bun --env-file=.env.dev --env-file=.env.dev.local --watch src/main.ts",
    "start:prod": "bun --env-file=.env.prod --env-file=.env.prod.local --watch src/main.ts",
    "build:dev": "bun --env-file=.env.dev --env-file=.env.dev.local bun.build.ts",
    "build:prod": "bun --env-file=.env.prod --env-file=.env.prod.local bun.build.ts",
    "preview:dev": "bun --env-file=.env.dev --env-file=.env.dev.local dist/main.js",
    "preview:prod": "bun --env-file=.env.prod --env-file=.env.prod.local dist/main.js",
    "db-init:dev": "bun --env-file=.env.dev --env-file=.env.dev.local src/db/init/index.ts",
    "db-init:prod": "bun --env-file=.env.prod --env-file=.env.prod.local src/db/init/index.ts",
    "db-migrate:dev": "bun --env-file=.env.dev --env-file=.env.dev.local drizzle-kit migrate",
    "db-migrate:prod": "bun --env-file=.env.prod --env-file=.env.prod.local drizzle-kit migrate",
    "db-studio:dev": "bun --env-file=.env.dev --env-file=.env.dev.local drizzle-kit studio",
    "db-studio:prod": "bun --env-file=.env.prod --env-file=.env.prod.local drizzle-kit studio",
    "db-gen:dev": "bun --env-file=.env.dev --env-file=.env.dev.local drizzle-kit generate",
    "db-gen:prod": "bun --env-file=.env.prod --env-file=.env.prod.local drizzle-kit generate",
    "check": "tsc && biome check --write --unsafe",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,jsonc,css,html}": [
      "tsc && biome check --write --unsafe"
    ]
  },
  "devDependencies": {
    "@types/bun": "^1.2.19",
    "drizzle-kit": "^0.31.4"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.0",
    "@youde/shared": "workspace:^",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.3",
    "elysia": "^1.4.9",
    "elysia-ip": "^1.0.10",
    "es-toolkit": "^1.39.10",
    "postgres": "^3.4.7",
    "zod": "^4.1.12"
  }
}
